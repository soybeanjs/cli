{
  "name": "template-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "vite",
    "lint": "oxlint --fix",
    "fmt": "oxfmt",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "upkg": "soy ncu"
  },
  "dependencies": {
    "@soybeanjs/ui": "0.5.3",
    "@unocss/reset": "66.6.0",
    "klona": "2.0.6",
    "pinia": "3.0.4",
    "vue": "3.5.27",
    "vue-router": "5.0.2"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.4.4",
    "@soybeanjs/unocss-preset": "0.2.0",
    "@types/node": "25.2.0",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitejs/plugin-vue-jsx": "5.1.4",
    "elegant-router": "1.1.2",
    "lint-staged": "16.2.7",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "simple-git-hooks": "2.13.1",
    "typescript": "5.9.3",
    "unocss": "66.6.0",
    "unplugin-vue-components": "31.0.0",
    "vite": "7.3.1",
    "vue-tsc": "3.2.4"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "oxlint --fix && oxfmt"
  },
  "packageManager": "pnpm@10.28.2"
}
