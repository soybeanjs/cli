{
  "name": "template-vue",
  "type": "module",
  "version": "0.8.3",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-s typecheck build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "commit": "soy git-commit",
    "cleanup": "soy cleanup",
    "update-pkg": "soy ncu",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "pinia": "2.1.7",
    "vue": "3.3.10",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@soybeanjs/cli": "0.8.2",
    "@soybeanjs/eslint-config": "1.0.5",
    "@types/node": "20.10.4",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "eslint": "8.55.0",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.3.3",
    "vite": "5.0.6",
    "vue-tsc": "1.8.25"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
