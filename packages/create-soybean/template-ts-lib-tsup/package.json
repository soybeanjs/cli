{
  "name": "ts-lib-tsup",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.3",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "cli-progress": "3.12.0",
    "consola": "3.4.0",
    "dayjs": "1.11.13",
    "execa": "9.5.2",
    "kolorist": "1.8.0",
    "ofetch": "1.4.1"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.2.0",
    "@soybeanjs/eslint-config": "1.6.0",
    "@types/cli-progress": "3.11.6",
    "@types/node": "22.13.10",
    "eslint": "9.22.0",
    "lint-staged": "15.5.0",
    "simple-git-hooks": "2.11.1",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
