{
  "name": "template-tsdown",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "cli-progress": "3.12.0",
    "consola": "3.4.2",
    "dayjs": "1.11.19",
    "execa": "9.6.1",
    "kolorist": "1.8.0",
    "ofetch": "1.5.1"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.4.4",
    "@soybeanjs/eslint-config": "1.7.5",
    "@types/cli-progress": "3.11.6",
    "@types/node": "25.2.0",
    "eslint": "9.39.2",
    "lint-staged": "16.2.7",
    "simple-git-hooks": "2.13.1",
    "tsdown": "0.20.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "packageManager": "pnpm@10.28.2"
}
