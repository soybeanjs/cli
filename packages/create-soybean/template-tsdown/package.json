{
  "name": "template-tsdown",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "cli-progress": "3.12.0",
    "consola": "3.4.2",
    "dayjs": "1.11.15",
    "execa": "9.6.0",
    "kolorist": "1.8.0",
    "ofetch": "1.4.1"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.4.0-beta.3",
    "@soybeanjs/eslint-config": "1.7.1",
    "@types/cli-progress": "3.11.6",
    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "lint-staged": "16.1.5",
    "simple-git-hooks": "2.13.1",
    "tsdown": "0.14.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
